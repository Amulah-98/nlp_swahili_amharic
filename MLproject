name: abtest

conda_env: conda.yaml

entry_points:
  load_data:
    command: "cd pipeline && python3 train.py"
  
  n_folds:
    parameters:
      n_folds: {type: int, default: 5}
    command: "cd pipeline && python3 cross_validator.py --n-folds {n_folds}"
  
  main:
    parameters:
      n_folds: {type: int, default: 5}
    command: "cd pipeline && python3 evaluate.py {n_folds}"